function chkvalid() {
   var v1 = document.userenter.form_account.value;
   var v2 = document.userenter.form_password.value;
   if (v1.length<=0)
      {
      alert("please enter all fields");
      return false;
      }
  else if (v2.length<=0)
      {
      alert("please enter all fields");
      return false;
      }
  else
    {
    return true;
    }
}
</script>
