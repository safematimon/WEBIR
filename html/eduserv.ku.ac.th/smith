<script language="javascript1.2">
	function makevisible(cur,which){
		strength=(which==0)? 1 : 0.5
		if (cur.style.mozopacity)
			cur.style.mozopacity=strength
		else if (cur.filters)
			cur.filters.alpha.opacity=strength*100
		}
</script>
