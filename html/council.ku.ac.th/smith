<script type="text/javascript" src="/assets/home/application-fc8ddd74cf0588755ecd9bcdbcbf764b.js" ></script>

<script>
    $(document).ready(function () {
        lineclamp();
    });

    $(window).resize(function () {
        lineclamp();
    });

    function lineclamp() {
        var tagp = $('.raw-description p');
        var lineheight = parsefloat(tagp.css('line-height'));
        var articleheight = $('.raw-description').height();
        var calc = parseint(articleheight / lineheight);
        tagp.css({
            "-webkit-line-clamp": "" + calc + ""
        });
    }
</script>

