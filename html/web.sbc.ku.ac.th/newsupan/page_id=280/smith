สัปดาห์นี้ : <span id="wvisits">_</span>
          <script type="text/javascript">

            var xmlhttp_wvisits = new xmlhttprequest();

            xmlhttp_wvisits.onreadystatechange = function() {
              if (xmlhttp_wvisits.readystate == 4 && xmlhttp_wvisits.status == 200) {
                document.getelementbyid("wvisits").innerhtml=xmlhttp_wvisits.responsetext;
              }
            }

            var url="http://web.sbc.ku.ac.th/newsupan/wp-content/plugins/newstatpress/includes/api/variables.php?var=wvisits&limit=0&flag=&url=";

            xmlhttp_wvisits.open("get", url, true);
            xmlhttp_wvisits.send();
          </script>
          คน 
