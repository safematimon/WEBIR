            var url="http://web.sbc.ku.ac.th/newsupan/wp-content/plugins/newstatpress/includes/api/variables.php?var=mvisits&limit=0&flag=&url=";

            xmlhttp_mvisits.open("get", url, true);
            xmlhttp_mvisits.send();
          </script>
          คน 
