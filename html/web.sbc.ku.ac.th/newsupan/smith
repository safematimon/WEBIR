            var xmlhttp_alltotalvisits = new xmlhttprequest();

            xmlhttp_alltotalvisits.onreadystatechange = function() {
              if (xmlhttp_alltotalvisits.readystate == 4 && xmlhttp_alltotalvisits.status == 200) {
                document.getelementbyid("alltotalvisits").innerhtml=xmlhttp_alltotalvisits.responsetext;
              }
