<script>
    $(document).ready(function () {
        $('#category').on('select2:selecting', function(e) {
            var category = e.params.args.data.id;
            if(category){
                window.location = category;
            }else{
                window.location = 'https://pr.ku.ac.th/web/news'
            }
        });
    });
</script>
    
