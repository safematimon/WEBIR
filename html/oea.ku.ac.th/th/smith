 <section  class="container"><div class="row">
	<div class="full-width-text spb_content_element col-sm-6 spb_text_column">
		<div class="spb_wrapper clearfix">
			
<h1 style="text-align: left;">นิสิต</h1>

		</div> 
	</div>  
	<div class="full-width-text spb_content_element col-sm-6 spb_text_column">
		<div class="spb_wrapper clearfix">
			
