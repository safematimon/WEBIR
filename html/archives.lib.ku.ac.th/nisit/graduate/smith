<script>

var otable;
$(function() {
    otable = $('#data-table').datatable({
        processing: true,
        serverside: true,
        // ordering: false,
        searching: false,
        blengthchange: false,
        info:     true,
        idisplaylength: 10,
          ajax: {
          url: 'https://archives.lib.ku.ac.th/nisit/graduate/datatable',
          type: "post",
          data: function ( d ) {
            d._token = "skk9kvyp5dzuqfjr5ywptvgm1u4fv75zyxs1094g";
            d.where={};
            $('.mywhere').each(function() {
              if( $.trim($(this).val()) && $.trim($(this).val()) != '0' ){
                d.where[$(this).attr('name')] = $.trim($(this).val());
              }
            });

            d.like={};
            $('.mylike').each(function() {
                if( $.trim($(this).val()) && $.trim($(this).val()) != '0' ){
                  d.like[$(this).attr('name')] = $.trim($(this).val());
                }
           
            });
      
            d.custom={};
            $('.mycustom').each(function() {
              if( $.trim($(this).val()) && $.trim($(this).val()) != '0' ){
                d.custom[$(this).attr('name')] = $.trim($(this).val());
              }
            });
            odata = d;
          },
        },

        order: [ 0 , "desc" ],
        columndefs : [ 
              {
              "targets": [0],
              "orderable": false,
              }
          ],
        columns: [
           {data: 'name', classname: 'text-left'},
            
           
        ],
        rowcallback: function(nrow, adata, dataindex){
         
        },
        statesave: true,
    });
    
    // $('.mywhere,.mylike,.mycustom').on('change', function(e){
    //     // alert("xxx");
    //     otable.draw();
    // });

    $('.btnsearch').on('click', function(e){

      e.preventdefault();

      var txtsearch = $(".txtsearch").val();

      if(txtsearch==""){
        $(".txtsearch").focus();
        return false;
      }

      $('#book_name').val(txtsearch);
      $('#spinner_frame').show();

      otable.draw();

      settimeout(function(){
        $("#spinner_frame").hide();
      }, 2500);

    });



});

            $('.overallgradceremony').owlcarousel({
                loop:true,
                margin:10,
                dots:true,
                loop:true,
                autoplay:true,
                responsiveclass:true,
                responsive:{
                    0:{
                        items:1,
                        nav:false
                    },
                    600:{
                        items:1,
                        nav:false
                    },
                    1200:{
                        items:1,
                        nav:true,
                    }
                }
            })

</script>
