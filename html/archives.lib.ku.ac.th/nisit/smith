<script>

var otable;
$(function() {
    otable = $('#data-table').datatable({
        processing: true,
        serverside: true,
        // ordering: false,
        searching: false,
        blengthchange: false,
        info:     true,
        idisplaylength: 10,
          ajax: {
          url: 'https://archives.lib.ku.ac.th/nisit/nisit/datatable',
          type: "post",
          data: function ( d ) {
            // console.log(d);
            d._token = "ft5fgwrcvpejkwvdwfult0xloscgsyqvkj1urkvt";
            d.where={};
            $('.mywhere').each(function() {
              if( $.trim($(this).val()) && $.trim($(this).val()) != '0' ){
                d.where[$(this).attr('name')] = $.trim($(this).val());
              }
            });

            d.like={};
            $('.mylike').each(function() {
                if( $.trim($(this).val()) && $.trim($(this).val()) != '0' ){
                  d.like[$(this).attr('name')] = $.trim($(this).val());
                }
           
            });
      
            d.like2={};
            $('.mylike2').each(function() {
                if( $.trim($(this).val()) && $.trim($(this).val()) != '0' ){
                  d.like2[$(this).attr('name')] = $.trim($(this).val());
                }
           
            });

            d.custom={};
            $('.mycustom').each(function() {
              if( $.trim($(this).val()) && $.trim($(this).val()) != '0' ){
                // d.custom['fullname'] = $.trim($(this).val());
                d.custom['ku_generation'] = $.trim($(this).val());
              }
            });
            odata = d;
          },
        },

        order: [[ 1, 'desc' ], [ 2, 'desc' ]],
        columndefs : [ 
              {
              "targets": [0],
              "orderable": false,
              }
          ],
        columns: [
            {data: 'qr',title :'<center>qr</center>', classname: 'text-center'},
            {data: 'name',title :'<center>ชื่อ-นามสกุล</center>', classname: 'text-left'},
            {data: 'ku_generation', title :'<center>ku รุ่น</center>', classname: 'text-center',render: function(d) {
                  if(d>0){
                   return d;
                  }else{
                    return '-';
                  }
            }},
            {data: 'board', title :'<center>คณะ</center>', classname: 'text-center',render: function(d) {
                   return (d?d:'-');
            }},
            {data: 'year_admission', title :'<center>ปี</center>', classname: 'text-center',render: function(d) {
                   return (d?d:'-');
            }},
            {data: 'detail',title :'<center>รายละเอียด</center>', classname: 'text-center'},
            
           
        ],
        rowcallback: function(nrow, adata, dataindex){
         
        },
        // statesave: true,
    });
    
    // $('.mywhere,.mylike,.mycustom').on('change', function(e){
    //     // alert("xxx");
    //     otable.draw();
    // });

    $('.btnsearch').on('click', function(e){

      e.preventdefault();
      var generation_desc = $('#generation_desc').val();
      var board_desc = $('#board_desc').val();
      var category_desc = $('#category_desc').val();
      var year_desc = $('#year_desc').val();
      console.log(generation_desc+":"+board_desc+":"+category_desc+":"+year_desc);

      var ch_select = false;
      if(
        (generation_desc!=null && generation_desc!='') || 
        (board_desc!=null && board_desc!='') || 
        (category_desc!=null && category_desc!='') ||
        (year_desc!=null && year_desc!='')
        ) {
        ch_select = true;
      }else{
        ch_select = false;
      }

      console.log(ch_select);

      var txtsearch = $(".txtsearch").val();

      if(ch_select == true && txtsearch==""){

        $(".txtsearch").val('%');
        $('#spinner_frame').show();
        otable.draw();

      }else{

          if(txtsearch==""){
            $(".txtsearch").focus();
            return false;
          }
          $('#spinner_frame').show();
          otable.draw();

      }

          settimeout(function(){
            $("#spinner_frame").hide();
          }, 2500);

    });



});
</script>

<script type="text/javascript">

        $(document).ready(function() {


                    $(document).on('keyup', '#keywords', function(event) {
                      event.preventdefault();
                      $('#generation_desc option:first').prop('selected',true).trigger( "change" );
                      $('#board_desc option:first').prop('selected',true).trigger( "change" );
                      $('#category_desc option:first').prop('selected',true).trigger( "change" );
                      $('#year_desc option:first').prop('selected',true).trigger( "change" );
                    });


                    $(document).on('change', '#keywords', function(event) {

                        event.preventdefault();
                        var str = $(this).val();
                        var res =  str.split(" ");
                        console.log(res.length);

                        if(res.length==1){
                           
                            $('#fname').removeclass("mylike").addclass("mylike2");
                            $('#sname').removeclass("mylike").addclass("mylike2");
                            $('#ku_generation').removeclass("mylike").addclass("mylike2");
                            $('#board').removeclass("mylike").addclass("mylike2");
                            $('#year_admission').removeclass("mylike").addclass("mylike2");

                            $('#fname').val($.trim(res[0]));
                            $('#sname').val($.trim(res[0]));
                            $('#ku_generation').val($.trim(res[0]));
                            $('#board').val($.trim(res[0]));
                            $('#year_admission').val($.trim(res[0]));

                        }else{

                            $('#fname').removeclass("mylike2").addclass("mylike");
                            $('#sname').removeclass("mylike2").addclass("mylike");
                            $('#ku_generation').removeclass("mylike2").addclass("mylike");
                            $('#board').removeclass("mylike2").addclass("mylike");
                            $('#year_admission').removeclass("mylike2").addclass("mylike");

                            $('#fname').val($.trim(res[0]));
                            $('#sname').val($.trim(res[1]));
                            $('#ku_generation').val($.trim(res[2]));
                            $('#board').val($.trim(res[3]));
                            $('#year_admission').val($.trim(res[4]));
                        }


                    });

                    $(document).on('keyup', '#keywords', function(event) {
                      $(".my_select").prop('selectedindex',0);
                    });

                    $(document).on('change', '#generation_desc', function(event) {
                        
                        var str = $('#keywords').val();
                        var res =  str.split(" ");

                        $('#fname').removeclass("mylike2").addclass("mylike");
                        $('#sname').removeclass("mylike2").addclass("mylike");
                        $('#ku_generation').removeclass("mylike2").addclass("mylike");
                        $('#board').removeclass("mylike2").addclass("mylike");
                        $('#year_admission').removeclass("mylike2").addclass("mylike");

                        $('#fname').val($.trim(res[0]));
                        $('#sname').val($.trim(res[1]));
                        $('#ku_generation').val($.trim(res[2]));
                        $('#board').val($.trim(res[3]));
                        $('#year_admission').val($.trim(res[4]));

                        $('#ku_generation').val($(this).val());
                    });

                    $(document).on('change', '#board_desc', function(event) {

                        var str = $('#keywords').val();
                        var res =  str.split(" ");

                        $('#fname').removeclass("mylike2").addclass("mylike");
                        $('#sname').removeclass("mylike2").addclass("mylike");
                        $('#ku_generation').removeclass("mylike2").addclass("mylike");
                        $('#board').removeclass("mylike2").addclass("mylike");
                        $('#year_admission').removeclass("mylike2").addclass("mylike");

                        $('#fname').val($.trim(res[0]));
                        $('#sname').val($.trim(res[1]));
                        $('#ku_generation').val($.trim(res[2]));
                        $('#board').val($.trim(res[3]));
                        $('#year_admission').val($.trim(res[4]));

                        $('#board').val($(this).val());
                    });

                    $(document).on('change', '#category_desc', function(event) {
                        var str = $('#keywords').val();
                        var res =  str.split(" ");

                        $('#fname').removeclass("mylike2").addclass("mylike");
                        $('#sname').removeclass("mylike2").addclass("mylike");
                        $('#ku_generation').removeclass("mylike2").addclass("mylike");
                        $('#board').removeclass("mylike2").addclass("mylike");
                        $('#year_admission').removeclass("mylike2").addclass("mylike");

                        $('#fname').val($.trim(res[0]));
                        $('#sname').val($.trim(res[1]));
                        $('#ku_generation').val($.trim(res[2]));
                        $('#board').val($.trim(res[3]));
                        $('#year_admission').val($.trim(res[4]));

                        $('#category').val($(this).val());
                    });

                    $(document).on('change', '#year_desc', function(event) {
                        var str = $('#keywords').val();
                        var res =  str.split(" ");

                        $('#fname').removeclass("mylike2").addclass("mylike");
                        $('#sname').removeclass("mylike2").addclass("mylike");
                        $('#ku_generation').removeclass("mylike2").addclass("mylike");
                        $('#board').removeclass("mylike2").addclass("mylike");
                        $('#year_admission').removeclass("mylike2").addclass("mylike");

                        $('#fname').val($.trim(res[0]));
                        $('#sname').val($.trim(res[1]));
                        $('#ku_generation').val($.trim(res[2]));
                        $('#board').val($.trim(res[3]));
                        $('#year_admission').val($.trim(res[4]));

                        $('#year_admission').val($(this).val());
                    });



                    $(document).on('click', '.btnqrcode', function(event) {

                        // event.preventdefault();

                           var id = $(this).data('id');
                           // alert(id);

                            $.ajax({
                                url: "https://archives.lib.ku.ac.th/nisit/asset/qrcode/show_qrcode.php ",
                                type: 'post',
                                datatype: 'html',
                                data: {data: id},
                            })
                            .done(function(data) {
                                $(".show_qrcode").html(data);
                            })

                    });


                    localstorage.clear();


        });
  
  </script>
