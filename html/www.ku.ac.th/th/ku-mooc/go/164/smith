  <script type="text/javascript">
  (function() {
    $(".register").click(function(event) {
      $("#class_enroll_form").submit();
      event.preventdefault();
    });



    $('#class_enroll_form').on('ajax:complete', function(event, xhr) {
      if(xhr.status == 200) {
        if (xhr.responsetext == "") {
          location.href = "/dashboard";
        }
        else {
          location.href = xhr.responsetext;
        }
      } else if (xhr.status == 403) {
          //location.href = "/register?course_id=course-v1:ku+ku001+2018&enrollment_action=enroll";
          // punsarn
          var login_url = "/auth/login/common-oauth2-backend/";
          location.href = login_url + "?next=" + encodeuricomponent(location.href);
      } else {
        $('#register_error').text(
            (xhr.responsetext ? xhr.responsetext : "เกิดข้อผิดพลาด กรุณาลองใหม่อีกครั้ง")
        ).css("display", "block");
      }
    });


  })(this)
  </script>

