<script type="text/javascript" src="www/js/myhttp.js"></script>
<script>
    var htmlloading = '<div class="c-content-panel" style="margin-bottom:0px;">';
    +'<div class="c-body">'
    +'  <div class="row">';
    +'    <div class="col-md-12">';
    +'      <div class="panel panel-default">';
    +'        <div class="panel-body c-center">';
    +'          <img src="www/images/loading.gif"><br>กำลังโหลดข้อมูล';
    +'         </div>';
    +'       </div>';
    +'     </div>';
    +'  </div>';
    +'</div>';

    function getlistposition(tab_id,job_type,page){
      var myhttp =  new  myhttp();
    	var tabcontent = document.getelementbyid(tab_id);
    	tabcontent.innerhtml = htmlloading;
    	myhttp.onresponsetext = function(text){
    		tabcontent.innerhtml = text;
    	}

      myhttp.addparameter("tab_id", tab_id);
    	myhttp.addparameter("page", page);
    	myhttp.addparameter("job_type", job_type);
    	myhttp.send("post", "/index/listposition");
    }

    function getlistcompany(tab_id,job_type,page){
      var myhttp =  new  myhttp();
    	var tabcontent = document.getelementbyid(tab_id);
    	tabcontent.innerhtml = htmlloading;
    	myhttp.onresponsetext = function(text){
    		tabcontent.innerhtml = text;
    	}

      myhttp.addparameter("tab_id", tab_id);
    	myhttp.addparameter("page", page);
    	myhttp.addparameter("job_type", job_type);
    	myhttp.send("post", "/index/listcompany");
    }

    function getlistnews(tab_id,page){
      var myhttp =  new  myhttp();
    	var tabcontent = document.getelementbyid(tab_id);
    	tabcontent.innerhtml = htmlloading;
    	myhttp.onresponsetext = function(text){
    		tabcontent.innerhtml = text;
    	}

    	myhttp.addparameter("page", page);
    	myhttp.send("post", "/index/listnews");
    }

    function getitemposition(tab_id,job_type,a_id){
      var myhttp =  new  myhttp();
    	var tabcontent = document.getelementbyid(tab_id);
    	tabcontent.innerhtml = htmlloading;
    	myhttp.onresponsetext = function(text){
    		tabcontent.innerhtml = text;
    	}

      myhttp.addparameter("tab_id", tab_id);
    	myhttp.addparameter("a_id", a_id);
    	myhttp.addparameter("job_type", job_type);
    	myhttp.send("post", "/index/listitemposition");
    }

    function getitemcompany(tab_id,job_type,p_name){
      var myhttp =  new  myhttp();
    	var tabcontent = document.getelementbyid(tab_id);
    	tabcontent.innerhtml = htmlloading;
    	myhttp.onresponsetext = function(text){
    		tabcontent.innerhtml = text;
    	}

      myhttp.addparameter("tab_id", tab_id);
    	myhttp.addparameter("p_name", p_name);
    	myhttp.addparameter("job_type", job_type);
    	myhttp.send("post", "/index/listitemcompany");
    }
</script>

