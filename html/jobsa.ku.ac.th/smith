            app.init(); // init core

            $('#login_button').click(function(){
                 var username = $('#login-account').val();
                 var password = $('#login-password').val();
                 if(username != '' && password != '' && $("#login")[0].checkvalidity())
                 {
                    $.ajax({
                       url:"/index/auth",
                       method:"post",
                       cache: false,
                       data: {username:username, password:password},
                       datatype: 'json',
                       success:function(response){
                         if(response.type == 'success'){
                           $('#login-form').hide();
                           location.reload();
                         } else {
                           $('#messages').removeclass().addclass('alert alert-danger').text(response.message);
                         }
                       }
                    });
                 }else{
                   $('#messages').removeclass().addclass('alert alert-warning').text("both fields are required");
                 }
            });

            $( "#login-password" ).keypress(function( event ) {
              if(event.which == 13) {
                 event.preventdefault();
                 $('#login_button').click();
              }
            });



            $('#signup_button').click(function(){
                 var email = $('#signup-email').val();
                 var user_name = $('#signup-username').val();
                 var user_pass = $('#signup-password').val();
                 var full_name = $('#signup-fullname').val();
                 var company = $('#signup-company').val();
                 var tel = $('#signup-tel').val();

                 if(email != '' && user_name != '' && user_pass != '' && full_name != '' && company != '' && tel != ''&& $("#signup")[0].checkvalidity())
                 {
                    $.ajax({
                       url:"/index/register",
                       method:"post",
                       cache: false,
                       data: {user_name:user_name,user_pass:user_pass,full_name:full_name,email:email,company:company,tel:tel},
                       datatype: 'json',
                       success:function(response){
                         if(response.type == 'success'){
                          //  $('#signup-form').hide();
                          //  location.reload();
                           $('#signup-messages').removeclass().addclass('alert alert-success').text(response.message);
                           $("#signup").closest('form').find("input[type=text],input[type=password],input[type=email]").val("");
                         } else {
                           $('#signup-messages').removeclass().addclass('alert alert-danger').text(response.message);
                         }
                       }
                    });
                 }else{
                   $('#signup-messages').removeclass().addclass('alert alert-warning').text("all fields are required");
                 }
            });

            getlistposition('tab-full-position','1','1');
            getlistposition('tab-part-position','2','1');
            getlistposition('tab-apprentice-position','3','1');
            getlistnews('tab-news-list','1');
          });
          function activatab(tab){
              $('.c-tab-icon-stack a[href="#' + tab + '"]').tab('show');
          };



        </script>
