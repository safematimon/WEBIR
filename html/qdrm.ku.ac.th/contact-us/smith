<script type="text/javascript">
    $.ajaxsetup({
        headers: {
            'x-csrf-token': $('meta[name="csrf-token"]').attr('content')
        }
    });
    $(".btn-submit").click(function(event){
  
        event.preventdefault();
        var name = $("#name").val();
        var email = $("#email").val();
        var subject = $("#subject").val();
        var message = $("#message").val();
        var verified = $("#g-recaptcha-response").val();

        function validateemail($email) {
            var emailreg = /^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})?$/;
            return emailreg.test( $email );
        }
        
        if(name === "" || name.length < 15)
        {
            toastr.error("กรุณาระบุ ชื่อ-นามสกุล ของท่านอย่างน้อย 15 ตัวอักษร !");
            toastr.options = {
                "closebutton": true,
                "newestontop": false,
                "progressbar": true,
                "positionclass": "toast-top-right",
                "preventduplicates": false,
                "onclick": null,
                "showduration": "300",
                "hideduration": "1000",
                "timeout": "5000",
                "extendedtimeout": "1000",
                "showeasing": "swing",
                "hideeasing": "linear",
                "showmethod": "fadein",
                "hidemethod": "fadeout"
            }
            return false;
            event.preventdefault();
        }
        else if(email === "" || !validateemail(email)) {
            toastr.error("กรุณาระบุ อีเมล์ ของท่านให้ถูกต้อง !");
            toastr.options = {
                "closebutton": true,
                "newestontop": false,
                "progressbar": true,
                "positionclass": "toast-top-right",
                "preventduplicates": false,
                "onclick": null,
                "showduration": "300",
                "hideduration": "1000",
                "timeout": "5000",
                "extendedtimeout": "1000",
                "showeasing": "swing",
                "hideeasing": "linear",
                "showmethod": "fadein",
                "hidemethod": "fadeout"
            }
            return false;
            event.preventdefault();
        }
        else if(subject === "" || subject.length < 15) {
            toastr.error("โปรดระบุ ชื่อเรื่อง และต้องมีความยาว อย่างน้อย 15 ตัวอักษร !");
            toastr.options = {
                "closebutton": true,
                "newestontop": false,
                "progressbar": true,
                "positionclass": "toast-top-right",
                "preventduplicates": false,
                "onclick": null,
                "showduration": "300",
                "hideduration": "1000",
                "timeout": "5000",
                "extendedtimeout": "1000",
                "showeasing": "swing",
                "hideeasing": "linear",
                "showmethod": "fadein",
                "hidemethod": "fadeout"
            }
            return false;
            event.preventdefault();
        }
        else if(message === "" || message.length < 50) {
            toastr.error("ข้อความของท่านจะต้องมีความยาวของประโยค อย่างน้อย 50 ตัวอักษร !");
            toastr.options = {
                "closebutton": true,
                "newestontop": false,
                "progressbar": true,
                "positionclass": "toast-top-right",
                "preventduplicates": false,
                "onclick": null,
                "showduration": "300",
                "hideduration": "1000",
                "timeout": "5000",
                "extendedtimeout": "1000",
                "showeasing": "swing",
                "hideeasing": "linear",
                "showmethod": "fadein",
                "hidemethod": "fadeout"
            }
            return false;
            event.preventdefault();
        }
        else if(verified.length === 0){
            toastr.error("please verified recaptcha now !!");
            toastr.options = {
                "closebutton": true,
                "newestontop": false,
                "progressbar": true,
                "positionclass": "toast-top-right",
                "preventduplicates": false,
                "onclick": null,
                "showduration": "300",
                "hideduration": "1000",
                "timeout": "5000",
                "extendedtimeout": "1000",
                "showeasing": "swing",
                "hideeasing": "linear",
                "showmethod": "fadein",
                "hidemethod": "fadeout"
            }
            return false;
            event.preventdefault();
        }
        
        else{
            $("#overlay").show();
            $.ajax({
                type:'post',
                url:"https://qdrm.ku.ac.th/contact-us",
                data:{fullname:name, email:email, subject:subject, message:message, token:verified},
                success: function(result){
                    $("#overlay").hide();
                    toastr.success("ส่งอีเมล์ เรียบร้อยแล้ว");
                    toastr.options = {
                        "closebutton": true,
                        "newestontop": false,
                        "progressbar": true,
                        "positionclass": "toast-top-right",
                        "preventduplicates": false,
                        "onclick": null,
                        "showduration": "300",
                        "hideduration": "1000",
                        "timeout": "5000",
                        "extendedtimeout": "1000",
                        "showeasing": "swing",
                        "hideeasing": "linear",
                        "showmethod": "fadein",
                        "hidemethod": "fadeout"
                    }
                    settimeout(function(){ 
                        window.location.href = '/contact-us';
                    }, 5000);
            }
            });
        }
        
    });
</script>

