    $("#formsubmit").submit(function () {
        var strsearch = $.trim($('#search').val().length);
        if (strsearch  < 5) {
            toastr.error("กรุณาระบุคำสืบค้นอย่างน้อย 5 ตัวอักษร !");
            toastr.options = {
                "closebutton": true,
                "newestontop": false,
                "progressbar": true,
                "positionclass": "toast-top-right",
                "preventduplicates": false,
                "onclick": null,
                "showduration": "300",
                "hideduration": "1000",
                "timeout": "5000",
                "extendedtimeout": "1000",
                "showeasing": "swing",
                "hideeasing": "linear",
                "showmethod": "fadein",
                "hidemethod": "fadeout"
            }
            return false;
            event.preventdefault();
        }
        else{
            $("#formsubmit").submit();
        }
    });
    $("#submit").on('click', function () {
        var strsearch = $("#search").val().length;
        if(strsearch < 5){
            toastr.error("กรุณาระบุคำสืบค้นอย่างน้อย 5 ตัวอักษร !");
            toastr.options = {
                "closebutton": true,
                "newestontop": false,
                "progressbar": true,
                "positionclass": "toast-top-right",
                "preventduplicates": false,
                "onclick": null,
                "showduration": "300",
                "hideduration": "1000",
                "timeout": "5000",
                "extendedtimeout": "1000",
                "showeasing": "swing",
                "hideeasing": "linear",
                "showmethod": "fadein",
                "hidemethod": "fadeout"
            }
            return false;
            event.preventdefault();
        }
        else{
            $("#formsubmit").submit();
        }
    });
</script>
