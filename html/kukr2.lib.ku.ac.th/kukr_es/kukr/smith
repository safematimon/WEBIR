    .loader {
        margin: 0 auto;
        border: 8px solid #9c3; /* thammsat yellow */
        border-top: 8px solid #066; /* thammasat red */
        border-radius: 50%;
        width: 40px;
        height: 40px;
        animation: spin 1.5s infinite;
    }

    @keyframes spin {
        0% {
            transform: rotate(0deg);
        }
        100% {
            transform: rotate(360deg);
        }
    }

    .hide{
        display: none;
    }
</style>
