            border: 1px solid rgba(0,0,0,.15);
            border-radius: .25rem;
        }

        a .nav-link_custom:hover .dropdown-menu_custom {
            display: block;
        }

        .mainmenu li:hover .submenu {
            display: block;
            max-height: 400px;
        }

        .submenu {
