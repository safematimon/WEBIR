<script type="text/javascript">
    $(document).ready(function () {

        $('.expand').click(function () {
            $('#fullchart').html('');
            element = $('#' + $(this).attr('rel'));
            $.fancybox.open(
                $('#full-wrap').html(),
                {
                    'transitionin': 'fade',
                    'transitionout': 'elastic',
                    'showclosebutton': true,
                    'scrolling': 'no',
                    aftershow: function () {
                        $('#full-wrap').html('');
                        renderbox = 'fullchart';
                        exporting = true;
                        show_data_label = true;
                        eval("cat = new array('" + element.attr('cat') + "');");
                        eval(element.attr('chart-type') + "('" + element.attr('title') + "', cat,'" + element.attr('data') + "', '');");
                    },
                    afterclose: function () {
                        $('#full-wrap').html('<div id="fullchart"></div>');
                    }
                }
            );
        });
    });
</script>

